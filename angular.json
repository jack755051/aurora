{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "aurora-toast": {
      "projectType": "library",
      "root": "projects/toast",
      "sourceRoot": "projects/toast/src",
      "prefix": "aurora-toast",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/toast/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/toast/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/toast/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/toast/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "probatio": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/probatio",
      "sourceRoot": "projects/probatio/src",
      "prefix": "aurora-probatio",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/probatio",
            "index": "projects/probatio/src/index.html",
            "browser": "projects/probatio/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/probatio/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/probatio/public"
              }
            ],
            "styles": ["projects/probatio/src/styles.scss"],
            "scripts": [],
            "server": "projects/probatio/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/probatio/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "probatio:build:production"
            },
            "development": {
              "buildTarget": "probatio:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/probatio/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/probatio/public"
              }
            ],
            "styles": ["projects/probatio/src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "ddb0fd2d-0e0e-4df4-af3d-ae02c7ed0154"
  }
}
